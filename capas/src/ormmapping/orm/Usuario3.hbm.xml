<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Usuario3" table="Usuario" lazy="false">
		<id name="uid" column="Uid" type="integer" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">seq_Usuario</param>
			</generator>
		</id>
		<property name="nickname" column="nickname" type="string" length="20" not-null="false" lazy="false">
			<meta attribute="field-description">
				apodo
			</meta>
		</property>
		<property name="password" column="password" type="string" length="30" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
